  },
  "workspaces": [
    "packages/*",
    "deployments/*",
    "docs"
  ],
  "version": "3.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "npm": ">=9.2.0"
