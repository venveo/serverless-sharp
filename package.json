{
  "name": "serverless-sharp",
  "description": "A Lambda function for performing on-demand image edits and manipulations.",
  "author": {
    "name": "Venveo",
    "url": "https://www.venveo.com",
    "email": "info@venveo.com"
  },
  "scripts": {
    "test": "jest",
    "docs": "serve ./docs",
    "build": "sls package",
    "deploy": "sls deploy",
    "make-json-schema": "quicktype ./data/schema.json -l schema -o ./data/json_schema.json",
    "make-schema-types": "quicktype -s schema ./data/json_schema.json -o src/types/imgix.d.ts --just-types --converters all-objects --all-properties-optional"
  },
  "version": "2.2.0",
  "private": false,
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.94",
    "@types/jest": "^27.1.4",
    "@types/node": "^17.0.25",
    "@types/sharp": "^0.30.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "aws-sdk": "^2.668.0",
    "aws-sdk-mock": "^5.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "jest": "^27.0.0",
    "prettier": "^2.6.2",
    "quicktype": "^15.0.261",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.26.2",
    "serverless-offline": "^8.0.0",
    "sharp": "0.30.4",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}
